<!-- Header with Background Image -->
<%= render "header" %>
<!-- Page Content -->
<div class="container">
  <div class="row row_custom">
    <h1>Contact Us</h1>
  </div>
  <div class="row">
    <hr>
  </div>
  <div class="row row_custom">
    <h3>If you want to get a demo, please leave your information bellow. We will contact you as soon as possible.</h3>
  </div>
  <div class="row row_custom">
    <%= bootstrap_form_with(model: @static_pages, local: true) do |f| %>
    <%= f.text_field :first_name %>
    <%= f.text_field :last_name %>
    <%= f.email_field :email %>
    <%= f.text_field :message %>
    <%= f.submit "Send" %>
    <% end %>
  </div>
</div>
<!-- /.row -->
</div>
<!-- /.container -->

<!-- Footer -->
<%= render "footer" %>
